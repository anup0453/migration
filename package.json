{
  "name": "helixai-gateway",
  "version": "2.0.0",
  "main": "index.js",
  "license": "none",
  "private": true,
  "author": "Florian Prechtl <florian.prechtl@siemens.com>",
  "scripts": {
    "test": "NODE_ENV=test tap",
    "test:win": "SET \"NODE_ENV=test\" && tap",
    "dev": "nodemon",
    "dev:win": "yarn nodemon --watch src/**/*.ts --ignore src/**/*.spec.ts --exec ts-node src/index.ts",
    "lint": "eslint src",
    "build": "yarn install && tsc",
    "fix": "yarn install && run-p \"lint:prettier --write\" \"lint:eslint --fix\" \"typecheck\"",
    "lint:prettier": "prettier \"**/*.{js,jsx,ts,tsx,css,scss,json,md,yml,html}\"",
    "lint:eslint": "eslint \"**/*.{ts,tsx}\"",
    "lint:all": "run-p \"lint:prettier\" \"lint:eslint\" \"typecheck\"",
    "typecheck": "npx tsc --noEmit --project ./tsconfig.json",
    "gen:mongo-types": "mtgen src/models/index.ts",
    "gen:schema": "ts-node src/scripts/createSchema",
    "gen:all": "mtgen src/models/index.ts && ts-node src/scripts/createSchema",
    "esbuild": "npx esbuild dist/src/index.js --bundle --minify --keep-names --log-level=error --tree-shaking=true --external:@fastify/swagger --external:@fastify/swagger-ui --outfile=dist/index.js --format=cjs --platform=node",
    "terser": "npx terser dist/index.js --compress --mangle --output dist/index.js",
    "build:all": "yarn build && yarn esbuild && yarn terser"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@arizeai/openinference-semantic-conventions": "1.0.0",
    "@azure/openai": "1.0.0-beta.12",
    "@azure/search-documents": "12.1.0-beta.2",
    "@azure/storage-blob": "12.18.0",
    "@fastify/auth": "^4.4.0",
    "@fastify/basic-auth": "^5.0.0",
    "@fastify/compress": "^6.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.1",
    "@fastify/swagger": "^8.5.1",
    "@fastify/swagger-ui": "^1.9.0",
    "@grpc/grpc-js": "^1.8.0",
    "@langchain/community": "^0.2.19",
    "@langchain/core": "^0.2.21",
    "@langchain/langgraph": "^0.0.33",
    "@langchain/openai": "^0.2.3",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.45.0",
    "@opentelemetry/resources": "^1.7.0",
    "@opentelemetry/sdk-trace-base": "^1.7.0",
    "@opentelemetry/sdk-trace-node": "^1.7.0",
    "app-root-path": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "dayjs": "^1.11.8",
    "dotenv": "^16.0.3",
    "fastify": "^4.18.0",
    "fs-extra": "^11.1.1",
    "http-errors": "^2.0.0",
    "ipaddr.js": "^2.2.0",
    "langchain": "^0.2.19",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "mongodb": "^6.13.0",
    "mongoose": "^8.9.5",
    "node-cron": "^3.0.3",
    "node-fetch": "2.6.7",
    "npm-run-all": "^4.1.5",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "qs": "^6.11.2",
    "snowflake-sdk": "2.0.4",
    "tiktoken": "1.0.15",
    "typescript-json-schema": "^0.62.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-ts": "^1.5.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.38",
    "@types/fs-extra": "^11.0.1",
    "@types/http-errors": "^2.0.4",
    "@types/lodash": "^4.14.195",
    "@types/mime": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "2.6.3",
    "@types/prettier": "^2.7.2",
    "@types/qs": "^6.9.7",
    "@types/snowflake-sdk": "^1.6.24",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "axios": "1.8.2",
    "esbuild": "^0.25.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-sort": "^0.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify-plugin": "^4.5.0",
    "mongoose-tsgen": "^9.2.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "tap": "^21.1.0",
    "terser": "^5.21.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.8.2",
    "yargs": "^17.7.2"
  },
  "tap": {
    "timeout": 180,
    "files": [
      "src/test/index.test.ts"
    ]
  }
}
